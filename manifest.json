{
  "name": "Asana task to Markdown",
  "version": "1.0",
  "description": "Convert selected Asana tasks into markdown url list",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "asana-logo.png"
  },
  "permissions": ["activeTab", "<all_urls>"]
}
